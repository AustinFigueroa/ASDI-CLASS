var Forum = function(obj){
	this.title = obj.title;
	this.url = obj.url;
	
	var db = Ti.Database.open("RedditGaming");
	var rows = db.execute("INSERT INTO posts () VALUES(?)", JSON.stringify(this));
	db.close();
};

exports.Object = Forum;

var del = function(id){
	var db = Ti.Database.open("statInfo");
	db.execute("DELETE FROM statTbl WHERE id=?", id);
	db.close();
	tblData = [];
	read();
};

exports.del = del;


